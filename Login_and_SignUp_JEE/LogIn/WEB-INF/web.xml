	<servlet>
		<servlet-name>loginservlet</servlet-name>
		<servlet-class>com.prudhvi.aj.LoginServlet</servlet-class>
		
	</servlet>
	<servlet-mapping>
		<servlet-name>loginservlet</servlet-name>
		<url-pattern>/loginservlet</url-pattern>
		
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>Login.html</welcome-file>
	</welcome-file-list>
