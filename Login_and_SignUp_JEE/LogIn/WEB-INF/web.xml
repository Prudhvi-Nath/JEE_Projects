		<filter-name>loginfilter</filter-name>
		<filter-class>com.prudhvi.aj.LoginFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>loginfilter</filter-name>
		<url-pattern>/loginservlet</url-pattern>
	</filter-mapping>
	<servlet>
